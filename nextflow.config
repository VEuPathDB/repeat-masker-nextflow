params {
  inputFilePath = "$baseDir/data/smallData.fa"
  fastaSubsetSize = 1
  trimDangling = false
  dangleMax = 0
  rmParams = "-xsmall"
  outputFileName = "blocked.seq"
  errorFileName = "blocked.err"
  outputDir = "$launchDir/output"
}
process {
  container = 'veupathdb/repeatmasker'
}
docker {
    enabled = true
}