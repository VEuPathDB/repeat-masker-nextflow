params {
  inputFilePath = "$baseDir/data/Data.fa"
  trimDangling = true
  dangleMax = 5
  outputFileName = "cleaned.fa"
  maxMemoryGigs = "2 GB"
  processorsPerNode = "1"
  container = 'veupathdb/repeatmasker:latest'
  executor = 'lsf'
  outputDir = "$launchDir/output"
  speciesName = ""
  numNodes = ""
}
process {
  container = 'veupathdb/repeatmasker:latest'
  executor = 'lsf'
  queue = 'eupathdb'
  maxForks = 20
}
#executor {
#  perJobMemLimit = 2GB
#}
docker {
    enabled = true
}
