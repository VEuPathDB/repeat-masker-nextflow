params {
  inputFilePath = "$baseDir/data/Data.fa"
  fastaSubsetSize = 100
  trimDangling = true
  dangleMax = 10
  rmParams = "-xsmall -species 'Cytauxzoon felis'"
  outputFileName = "blocked.seq"
  errorFileName = "blocked.err"
  outputDir = "$launchDir/output"
}
process {
  container = 'veupathdb/repeatmasker:latest'
}
docker {
    enabled = true
}
