params {
  inputFilePath = "$baseDir/data/Data.fa"
  fastaSubsetSize = 1
  trimDangling = false
  dangleMax = 0
  rmParams = "-xsmall"
  outputFileName = "blocked.seq"
  errorFileName = "blocked.err"
  outputDir = "$launchDir/output"
}
process {
  container = 'veupathdb/repeatmasker:latest'
}
docker {
    enabled = true
}
// TODO
// rmParams "-species" will not work due to dfam files not containing full library. These would be to large to include in container. The solution will be to convert the specified library files into fasta format to allow a path to them to be specified with the repeatmasker argument "-lib". 